{
    "name": "web-a1/bitrix-vsepeski",
    "description": "Custom Bitrix24 integration module platform for vsepeski.ru",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "B24\\\\Center\\\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "B24\\\\Center\\\\Tests\\\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "db:migrate": "@php bin/migrate"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
